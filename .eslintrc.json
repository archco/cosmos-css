{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "globals":{
    "jQuery": true,
    "$": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-console": "off"
  }
}
