-
	num = ['one', 'two', 'three', 'four', 'five', 'six', 'seven', 'eight', 'nine', 'ten']

doctype html
html
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible' content='IE=edge')
		meta(name='viewport' content='width=device-width, initial-scale=1.0')
		block title
			title "Default title"
		link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
		link(rel="stylesheet" href="../dist/css/style.css")
	body
		header(class='bg-primary')
			nav
				div(class='nav-header')
					a(class='nav-brand' href='#') Brand Logo
					button(class='menu-toggle' type='button')
						span(class='hide') Toggle Navigation
						div(class='icon-bar1')
						div(class='icon-bar2')
						div(class='icon-bar3')
				div(class='menu-wrapper')
					ul(class='menu-left')
						li
							a(href='test.html') test
						li(class='active')
							a(href='#') active
						- for (var i = 0; i < 3; i++)
							li 
								a(href='#')= 'link ' + num[i]
						li(class='dropdown')
							button(class='dropdown-toggle') dropdown 
								i(class='fa fa-caret-down' aria-hidden='true')
							ul(class='dropdown-content')
								li
									a(href='#') link1
								li
									a(href='#') link2
								li
									a(href='#') link3
					ul(class='menu-right')
						li
							a(href='#') Login
						li
							a(href='#') Register

		block content

		footer
			div(class='container')
				div(class='row')
					div(class='col-6')
						h3 Footer content
						p footer content here..
					div(class='col-6')
						h3 Links
						p
							a(href='#') Link1
							a(href='#') Link2
							a(href='#') Link3
			div(class='footer-copyright')
				|copyright &copy; 2016 cosmos all rights reserved.
		script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js')
		<script src="https://cdnjs.cloudflare.com/ajax/libs/babel-polyfill/6.16.0/polyfill.min.js"></script>
		script(src='../dist/js/script.js')