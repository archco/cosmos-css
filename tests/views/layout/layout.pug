-
    num = ['one', 'two', 'three', 'four', 'five', 'six', 'seven', 'eight', 'nine', 'ten']

doctype html
html
    head
        meta(charset='utf-8')
        block title
            title "Default title"
        link(rel="stylesheet" href="../dist/css/style.css")
    body
        header(class='bg-primary')
            nav
                div(class='nav-header')
                    a(class='nav-brand' href='#') Brand Logo
                    button(class='menu-toggle' type='button')
                        span(class='hide') Toggle Navigation
                        div(class='icon-bar1')
                        div(class='icon-bar2')
                        div(class='icon-bar3')
                div(class='menu-wrapper')
                    ul(class='menu-left')
                        li
                            a(href='test.html') test
                        li(class='active')
                            a(href='#') active
                        - for (var i = 0; i < 3; i++)
                            li 
                                a(href='#')= 'link ' + num[i]
                    ul(class='menu-right')
                        li
                            a(href='#') Login
                        li
                            a(href='#') Register

        block content

        footer
